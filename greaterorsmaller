#include<iostream.h>
using namespace std;

int main(){
int num1,num2;
cout<<"Enter the first integer";
cin>>num1;
cout<<"Enter the second integer";
cin>>num2;
if(num1 > num2){
  cout<<"The first integer is greater."<<endl;
  }
 else if(num2 > num1){
  cout<<"The second integer is greater."<<endl;
}
else{
  cout<<"Both are equal."<<endl;
}
return 0;
}
